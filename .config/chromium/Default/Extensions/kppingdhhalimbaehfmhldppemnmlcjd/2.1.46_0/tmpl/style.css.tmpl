{{fontFace
@font-face {
	font-family: '{{fontFamily}}';
	src:url('{{fontsDir}}{{fontFamily}}.eot{{cacheBuster}}');
	src:url('{{fontsDir}}{{fontFamily}}.eot?#iefix{{hash}}') format('embedded-opentype'),
		url('{{fontsDir}}{{fontFamily}}.ttf{{cacheBuster}}') format('truetype'),
		url('{{fontsDir}}{{fontFamily}}.woff{{cacheBuster}}') format('woff'),
		url('{{fontsDir}}{{fontFamily}}.svg{{cacheBuster}}#{{fontFamily}}') format('svg');
	font-weight: normal;
	font-style: normal;
}
}}{{fontFaceB64
@font-face {
	font-family: '{{fontFamily}}';
	src: url('{{fontsDir}}{{fontFamily}}.eot');
}
@font-face {
	font-family: '{{fontFamily}}';
	src: url(data:application/x-font-ttf;charset=utf-8;base64,{{ttf}}) format('truetype'),
		 url(data:application/font-woff;charset=utf-8;base64,{{woff}}) format('woff');
	font-weight: normal;
	font-style: normal;
}
}}
{{selector}} {
	font-family: '{{fontFamily}}';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;
{{liga
	{{liga}}
	/* Enable Ligatures ================ */
	-webkit-font-feature-settings: "liga";
	-moz-font-feature-settings: "liga=1";
	-moz-font-feature-settings: "liga";
	-ms-font-feature-settings: "liga" 1;
	-o-font-feature-settings: "liga";
	font-feature-settings: "liga";
}}
	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

{{glyphs
.{{prefix}}{{glyphName}}{{postfix}}:before {
	content: "\{{code}}";
}
}}